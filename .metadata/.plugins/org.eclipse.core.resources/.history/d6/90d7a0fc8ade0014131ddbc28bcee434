package edu.hm.cs.vss;

public class Fork {

	public final int nr;
	private boolean inUse = false;
	
	public Fork(int nr) {
		this.nr = nr;
	}
	
	public boolean isInUse() {
		return inUse;
	}
	
	public void take() {
		
	}
	
	public void release() {
		
	}
	
	public String toString() {
		return ""+nr;
	}
}
