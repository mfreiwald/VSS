package edu.hm.css.vss;

public class Main {

	final int PRODUCERS = 3;
	final int CONSUMERS = 10;
	
	public static void main(String[] args) {
		Data data = new Data();
		
		for(int i=0; i<PRODUCERS; i++) {
			Producer pro = new Producer(i, data);
			pro.start();
		}
		Consumer con = new Consumer(data);
		
		pro.start();
		con.start();
	}
}
