package edu.hm.css.vss;
import java.util.ArrayList;


public class Data {

	ArrayList<Long> list = new ArrayList<Long>();
	
	public void writeData(long i) {
		System.out.println("Write Data "+i);
		list.add(i);
	}
	
	public long readData() {
		if(list.size() == 0) {
			return -1;
		}
		long i = list.get(list.size()-1);
		System.out.println("Read Data "+i);
		return i;
	}
	
}
